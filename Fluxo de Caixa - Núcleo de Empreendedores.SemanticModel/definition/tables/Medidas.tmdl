	measure 'Movimentação - Data Quebra' = ```
			
			VAR _SaldoRisco = [Movimentacao Liquida SOB RISCO] // Medida que você já criou.
			
			RETURN
			    // Retorna a menor data (a primeira) que atende ao critério de risco.
			    MINX( 
			        // 1. Itera sobre todas as datas no contexto
			        FILTER( 
			            ALL(d_Calendario[Data]), 
			            // 2. Filtra apenas as datas onde o Saldo Sob Risco é negativo
			            [Movimentacao Liquida SOB RISCO] < 0 
			        ), 
			        // 3. Retorna a data (o contexto da linha atual)
			        d_Calendario[Data] 
