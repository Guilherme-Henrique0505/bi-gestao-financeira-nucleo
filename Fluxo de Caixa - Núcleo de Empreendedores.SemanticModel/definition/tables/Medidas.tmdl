	measure 'Movimentacao Liquida SOB RISCO' = ```
			
			VAR DiasAtraso = [Atraso de Receita dias]
			VAR EntradasAtrasadas = 
			    CALCULATE(
			        [Total a Receber],
			        DATEADD( d_Calendario[Data], DiasAtraso, DAY )
			    )
			
			VAR SaidasSemAtraso = [Total a Pagar]
			
			RETURN
			    EntradasAtrasadas - SaidasSemAtraso
			```
		formatString: "R$"\ #,0.###############;-"R$"\ #,0.###############;"R$"\ #,0.###############
